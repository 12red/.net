
<html5>
    <head>

        <title>
            Edit your story
        </title>
        <link rel="stylesheet" href="~/fontes/material-icon/css/material-design-iconic-font.min.css">

        <!-- Main css -->
        <link rel="stylesheet" href="~/csses/style.css">

    </head>
    <body>
        <div class="main">

            <div class="container">
                @model Venting
                <form asp-action="writing" style="background-color:whitesmoke;">
                    <div class="form-group form-input">
                        <input type="hidden" asp-for="id" />
                    </div>
                    <div class="form-group form-input">
                        <textarea style=" background-color:white;width:1000px; height:250px; color:black; top:20px" vocab="http://data-vocabulary.org"  asp-for="secret">
</textarea>
                      
                    </div>
                        <input  style="background-color:salmon;" type="submit" value="Edit" class="btn btn-default" />

                </form>
                </div>
            </div>

    </body>


</html5>


